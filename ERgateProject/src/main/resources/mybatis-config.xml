<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
   
   <settings>
      <setting name="jdbcTypeForNull" value="NULL"/>
   </settings>
   
   <typeAliases>
      <typeAlias type="com.kh.ergate.main.model.vo.Employee" alias="Employee"/>
      <typeAlias type="com.kh.ergate.main.model.vo.Department" alias="Department"/>
      <typeAlias type="com.kh.ergate.main.model.vo.Rank" alias="Rank"/>
      <typeAlias type="com.kh.ergate.main.model.vo.Job" alias="Job"/>
      <typeAlias type="com.kh.ergate.board.model.vo.Board" alias="Board" />
      <typeAlias type="com.kh.ergate.board.model.vo.Reply" alias="Reply" />
      <typeAlias type="com.kh.ergate.board.model.vo.ReReply" alias="ReReply" />
